							"source": {
								"type": "SqlServerSource",
								"sqlReaderQuery": {
									"value": "select count(1) as row_count from dbo.customer",
									"type": "Expression"
								},
								"queryTimeout": "02:00:00",
								"partitionOption": "None"
							},
							"dataset": {
								"referenceName": "Sql_Product",
								"type": "DatasetReference",
								"parameters": {
									"schema_name": ".",
									"table_name": "."
								}
							}
						}
					},
					{
						"name": "If is greater than 600",
						"type": "IfCondition",
						"dependsOn": [
