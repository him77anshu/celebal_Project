		{
			"name": "[concat(parameters('factoryName'), '/pipeline_customer')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "GetCustomer",
						"type": "Lookup",
